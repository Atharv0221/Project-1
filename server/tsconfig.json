{
  "compilerOptions": {
    "target": "es2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "skipLibCheck": true,
    "types": [
      "node",
      "express",
      "multer",
      "jsonwebtoken",
      "cors"
    ],
    "outDir": "./dist",
    "rootDir": "./"
  }
}